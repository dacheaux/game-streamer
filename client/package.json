{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/auth/*": {
      "target": "http://localhost:5020"
    },
    "/social/*": {
      "target": "http://localhost:5020"
    },
    "/local/*": {
      "target": "http://localhost:5020"
    },
    "/api/*": {
      "target": "http://localhost:5020"
    }
  },
  "dependencies": {
    "async": "^2.6.0",
    "axios": "^0.17.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "raven-for-redux": "^1.1.1",
    "raven-js": "^3.20.1",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.0",
    "react-dom": "^16.2.0",
    "react-ellipsis-with-tooltip": "^1.0.6",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-modal": "^1.3.0",
    "react-scripts": "1.0.17",
    "react-timeout": "^1.0.1",
    "react-tooltip": "^3.4.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "twitch-embed": "^1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "webp": "webpack-dev-server",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-all": "npm-run-all -p watch-css start",
    "build-all": "npm-run-all build-css build"
  }
}
